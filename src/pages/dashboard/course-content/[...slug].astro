---
import Layout from '../../../layouts/Layout.astro';
import { CourseContent } from '../../../components/dashboard/course-content';
export async function getStaticPaths() {
  return [];
}
export const prerender = false;
const rawSlug = (Astro.params as any).slug;
const courseId = Array.isArray(rawSlug) ? rawSlug[0] : rawSlug;
---
<Layout>
  <link rel="stylesheet" href="/src/styles/course-content.css" />
  <CourseContent client:load courseId={courseId} />
</Layout>
