---
import Layout from '../../../layouts/Layout.astro';
import StudentSessionView from '../../../components/StudentSessionView.tsx';
export async function getStaticPaths() {
  // Fetch all courses for static generation (or return empty for SSR only)
  return [];
}
export const prerender = false;
const { slug } = Astro.params;
let courseId = '';
let sessionId = '';
if (Array.isArray(slug)) {
  courseId = slug[0] || '';
  sessionId = slug[1] || '';
} else if (typeof slug === 'string') {
  // If slug is a string, try to split it (shouldn't happen, but fallback)
  [courseId, sessionId] = (slug as string).split('/')
}
---
<Layout>
  <StudentSessionView client:load courseId={courseId} sessionId={sessionId} />
</Layout>
